<div id="leftSide">

		<img src="<%= Hud.find(@behandlinger.hud_id).picture %>" alt="bilde"/>
	<%= link_to 'Bestil time', "/kontakt", :class => 'button'%>
	</div>
<div id="centerSection">
	<h3><%= @behandlinger.title %></h3>
	<%= simple_format @behandlinger.content %>
</div>
<div id="rightSide">
	
	<div id="linkMargin">
	<% hud = Hud.find(@behandlinger.hud_id) %>
	<%= link_to hud.title, hud %>
	<% hud.behandlingers.each do |b|%>
	<% if b == @behandlinger %>
		<%= link_to b.title, b, :class => 'at' %>
	<% else %>
	<%= link_to b.title, b, :class => 'other' %>
	<% end %>
	<% end %>
</div>
	
	
	



</div>